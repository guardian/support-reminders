  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/pg": "^7.4.14",
    "@types/pg-format": "^1.0.2",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1415.0",
    "csv-writer": "^1.6.0",
    "node-fetch": "^2.1.2",
    "pg": "8.4.0",
    "pg-format": "^1.0.4",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@guardian/eslint-config": "^0.4.2",
    "@guardian/eslint-config-typescript": "^0.4.2",
    "@guardian/prettier": "^0.4.2",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.12",
