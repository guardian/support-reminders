{
	"compilerOptions": {
		"target": "ES2021",
		"module": "commonjs",
		"sourceMap": true,
		"strict": true,
		"allowJs": true,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"outDir": "./target",
		"esModuleInterop": true
	},
	"include": [
		"./src/**/*.ts",
		"./**/gen-nodejs/*.js"
	]
}
